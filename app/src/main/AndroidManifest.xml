<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs.inhatc.sensor.smart_sensor_project">

    <uses-permission android:name="android.permission.INTERNET"/> <!--인터넷 사용-->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/> <!--위치정보 활용-->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/> <!--위치정보 활용-->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/> <!--WIFI 상태활용-->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/> <!--WIFI 상태활용-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".KakaoMapViewer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WalkingViewer">
        </activity>
        <activity android:name=".SearchViewer">
        </activity>
    </application>

    <meta-data
        android:name="com.kakao.sdk.Appkey"
        android:value="ad85475be6af5302b83e3ef713460b83"/>
</manifest>